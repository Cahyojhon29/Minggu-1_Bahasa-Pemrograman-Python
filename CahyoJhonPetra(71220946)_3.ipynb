        "import math\n",
        "uang_awal = 200000000\n",
        "uang_minimal = 400000000\n",
        "bunga = 10\n",
        "perkembangan_bunga = 1 + bunga / 100\n",
        "waktu = math.log(uang_minimal / uang_awal) / math.log(perkembangan_bunga)\n",
        "print(\"Waktu yang dibutuhkan:\", waktu, \"tahun\")\n"
